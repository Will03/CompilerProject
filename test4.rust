/* Sigma.rust
 *
 * Compute sum = 1 + 2 + ... + n
 */

fn main() {
  // variables
  let n = 10;
  let mut sum = 0;
  let mut index:int = 1;
  let mut index2:int = 1;
  while (index <= n) {
    index2 = 0;
    while (index2 <= n) {
      sum = sum + index;
      index2 = index2 + 1;
      
  } 
  index = index + 1;
  } 
  print "The sum is ";
  println sum;
}
